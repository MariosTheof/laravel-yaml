{
    "name": "athmarios/yaml-response",
    "description": "YAML parser for Laravel 8.",
    "keywords": [
        "yaml",
        "laravel-yaml",
        "laravel",
        "laravel-8",
        "laravel-7",
        "api",
        "response",
        "responses",
        "response-type"
    ],
    "homepage": "https://github.com/MariosTheof/laravel-yaml",
    "license": "MIT",
    "authors": [
        {
            "name": "Dukagjin Surdulli",
            "email": "dukagjin.surdulli@gmail.com",
            "homepage": "https://dukagj.in",
            "role": "Developer"
        },
        {
            "name": "Marios Athanasiadis",
            "email": "athmarios@agenso.gr",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "symfony/yaml": "^5.2.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0",
        "orchestra/testbench": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Dugajean\\Yaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dugajean\\Yaml\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dugajean\\Yaml\\YamlServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
